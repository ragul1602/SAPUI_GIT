<mvc:View controllerName="com.smax.odata.ZPROJECT_DEMO12.controller.ODATA" xmlns:mvc="sap.ui.core.mvc"
	xmlns:html="http://www.w3.org/1999/xhtml" displayBlock="true" xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
					<List items="{/Customers}">
						<items>
							<StandardListItem title="{CompanyName}" description="{CustomerID}"/>
						</items>
					</List>
				</content>
				
	
			</Page>
		</pages>
	</App>
</mvc:View>