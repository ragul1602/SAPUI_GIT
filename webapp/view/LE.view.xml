<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="COM.SMAX.LE.ZEXP_LAYOUTEDITOR.controller.LE" displayBlock="true">
<Shell id="shell">
		<App id="app">
			<pages>
	<Page id="page" title="{i18n>title}">
				<content>
				    <Table items="/Products" noDataText="Drop column list items here and columns in the area above" id="table0">
				        <items>
				            <ColumnListItem type="Active" id="item0">
				                <cells>
				                    <Text text="{ProductID}" id="text0"/>
				                    <Text text="{ProductName}" id="text1"/>
				                    <Text text="{UnitPrice}" id="text2"/></cells>
				            </ColumnListItem>
				        </items>
				        <columns>
				            <Column id="column0">
				                <header>
				                    <Label text="Header 1" id="label0"/>
				                </header>
				            </Column>
				            <Column id="column1">
				                <header>
				                    <Label text="Header 2" id="label1"/>
				                </header>
				            </Column>
				            <Column id="column2">
				                <header>
				                    <Label text="Header 3" id="label2"/>
				                </header>
				            </Column></columns></Table>
				</content>
				</Page>
	
			</pages>
		</App>
	</Shell>
</mvc:View>