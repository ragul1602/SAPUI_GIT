{
	"_version": "1.8.0",
	"sap.app": {
		"id": "emax.cls05.ZCLS05SPLITAPP",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZGW_CLS05_PRODUCT_SRV": {
				"uri": "/sap/opu/odata/sap/ZGW_CLS05_PRODUCT_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"Northwind.svc": {
				"uri": "/Northwind/v2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"localUri": "localService/Northwind.svc/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "emax.cls05.ZCLS05SPLITAPP.view.SAPP",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "emax.cls05.ZCLS05SPLITAPP.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Northwind.svc",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "emax.cls05.ZCLS05SPLITAPP.view",
				"controlAggregation": "pages",
				"controlId": "idSplitApp",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "home",
				"pattern": "",
				"target": ["ProductList", "ProductDetails"]
			}, {
				"name": "route name",
				"pattern": "details",
				"titleTarget": "",
				"greedy": false,
				"target": ["ProductList", "ProductDetails"]
			}],
			"targets": {
				"ProductList": {
					"viewType": "XML",
					"viewName": "ProductList",
					"controlAggregation": "masterPages"
				},
				"ProductDetails": {
					"viewType": "XML",
					"viewName": "ProductDetails",
					"controlAggregation": "detailPages"
				}
			}
		}
	}
}